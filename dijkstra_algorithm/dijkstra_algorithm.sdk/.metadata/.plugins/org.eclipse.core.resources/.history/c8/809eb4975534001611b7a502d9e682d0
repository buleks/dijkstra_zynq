#ifndef CONFIG_H_
#define CONFIG_H_

#define N 8

#define MEM_BASE_ADDR		0x01000000
#define TX_BUFFER_BASE		(MEM_BASE_ADDR + 0x00100000)
#define RX_BUFFER_BASE		(MEM_BASE_ADDR + 0x00300000)
#define RX_BUFFER_HIGH		(MEM_BASE_ADDR + 0x004FFFFF)



const uint8_t START_POINT = 0;

#endif /* CONFIG_H_ */
